        - 
          id: 14
          name: "CrossSection"
          plugin: PHITS
          class: CrossSectionItem
          is_checked: true
          data: 
            code: 0
            maxcas: 1000
            maxbch: 2
            nx: 10
            ny: 10
            nz: 3
            ne: 1
            xmin: -5
            ymin: -5
            zmin: 0
            emin: 0
            xmax: 5
            ymax: 5
            zmax: 3
            emax: 2
