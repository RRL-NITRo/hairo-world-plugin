items: 
  id: 0
  name: "RootItem"
  plugin: Base
  class: RootItem
  is_builtin_item: true
  children: 
    - 
      id: 1
      name: "World"
      plugin: Body
      class: WorldItem
      is_checked: true
      data: 
        collision_detection: false
        collision_detector: AISTCollisionDetector
        default_material_table_file: "${SHARE}/default/materials.yaml"
      children: 
        - 
          id: 2
          name: "Tank"
          plugin: Body
          class: BodyItem
          is_checked: true
          data: 
            file: "../model/Tank.body"
            currentBaseLink: "CHASSIS"
            format: CHOREONOID-BODY
            rootPosition: [ -1.5, 1, 0.105900008 ]
            rootAttitude: [ 
              1, 2.52504595e-09, -9.5407115e-09, 
              -2.52504596e-09, 1, -2.29087761e-10, 
              9.5407115e-09, 2.29087785e-10, 1 ]
            jointDisplacements: [ 1.15637e-10, -2.21112, 0, 0 ]
            initialJointDisplacements: [ 0, -1.64966, 0, 0 ]
            jointPositions: [ 2.01825e-12, -0.0385913, 0, 0 ]
            initialRootPosition: [ -1.5, 1, 0.1004 ]
            initialRootAttitude: [ 
              1, 0, 0, 
              0, 1, 0, 
              0, 0, 1 ]
            initialJointPositions: [ 0, -0.028792, 0, 0 ]
            staticModel: false
            collisionDetection: true
            selfCollisionDetection: false
            location_editable: true
            scene_sensitive: true
            zmp: [ 0, 0, 0 ]
          children: 
            - 
              id: 3
              name: "JoystickController"
              plugin: Body
              class: SimpleControllerItem
              data: 
                isNoDelayMode: true
                controllerOptions: ""
                controller: "TankJoystickController"
                base_directory: "Controller directory"
                reloading: true
                export_symbols: false
            - 
              id: 4
              name: "DoseMeterController"
              plugin: Body
              class: SimpleControllerItem
              data: 
                isNoDelayMode: false
                controllerOptions: ""
                controller: "DoseMeterController"
                base_directory: "Controller directory"
                reloading: false
                export_symbols: false
            - 
              id: 5
              name: "GammaImager"
              plugin: PHITS
              class: GammaImagerItem
              data: 
                sub_items: 
                  - 
                    class: GammaImageVisualizerItem
                    name: PinholeCamera
                    is_checked: true
                    mode: 0
                    min: 1
                    max: 10
                    num_filters: 3
                    filter_0: false
                    filter_1: false
                    filter_2: false
                    maxcas: 100000
                    maxbch: 10
                    put_messages: true
                    default_nuclide_table_file: ${SHARE}/default/nuclides.yaml
                    default_element_table_file: ${SHARE}/default/elements.yaml
                  - 
                    class: GammaImageVisualizerItem
                    name: ComptonCamera
                    is_checked: true
                    mode: 0
                    min: 22023
                    max: 100000
                    num_filters: 3
                    filter_0: false
                    filter_1: false
                    filter_2: false
                    maxcas: 1000000
                    maxbch: 10
                    put_messages: true
                    default_nuclide_table_file: ${SHARE}/default/nuclides.yaml
                    default_element_table_file: ${SHARE}/default/elements.yaml
        - 
          id: 6
          name: "AISTSimulator"
          plugin: Body
          class: AISTSimulatorItem
          is_checked: true
          data: 
            timeStep: 0.001
            realtimeSync: true
            recording: "off"
            timeRangeMode: "Unlimited"
            timeLength: 180
            active_control_time_range_mode: false
            allLinkPositionOutputMode: false
            deviceStateOutput: true
            controllerThreads: true
            recordCollisionData: false
            controllerOptions: ""
            scene_view_edit_mode_blocking: false
            dynamicsMode: "Forward dynamics"
            integrationMode: "Runge Kutta"
            gravity: [ 0, 0, -9.80665 ]
            min_friction_coefficient: 0
            max_friction_coefficient: 100
            cullingThresh: 0.005
            contactCullingDepth: 0.05
            errorCriterion: 0.001
            maxNumIterations: 1000
            contactCorrectionDepth: 0.0001
            contactCorrectionVelocityRatio: 1
            kinematicWalking: false
            2Dmode: false
            oldAccelSensorMode: false
          children: 
            - 
              id: 7
              name: "GLVisionSimulator"
              plugin: Body
              class: GLVisionSimulatorItem
              data: 
                enabled: true
                targetSensors: [ PinholeCamera, ComptonCamera ]
                maxFrameRate: 1000
                maxLatency: 1
                recordVisionData: true
                threadMode: Sensor
                bestEffort: false
                allSceneObjects: false
                rangeSensorPrecisionRatio: 2
                depthError: 0
                enableHeadLight: true
                enableAdditionalLights: true
                antiAliasing: false
            - 
              id: 8
              name: "DoseSimulator"
              plugin: PHITS
              class: DoseSimulatorItem
              data: 
                enabled: true
                default_shield_table_file: "${SHARE}/default/shields.yaml"
        - 
          id: 9
          name: "Floor"
          plugin: Body
          class: BodyItem
          data: 
            file: "../model/floor.body"
            currentBaseLink: "Floor"
            format: CHOREONOID-BODY
            rootPosition: [ 0, 0, -0.1 ]
            rootAttitude: [ 
              1, 0, 0, 
              0, 1, 0, 
              0, 0, 1 ]
            jointPositions: [  ]
            initialRootPosition: [ 0, 0, -0.1 ]
            initialRootAttitude: [ 
              1, 0, 0, 
              0, 1, 0, 
              0, 0, 1 ]
            staticModel: true
            collisionDetection: true
            selfCollisionDetection: false
            location_editable: false
            scene_sensitive: true
            zmp: [ 0, 0, 0 ]
        - 
          id: 10
          name: "box1"
          plugin: Body
          class: BodyItem
          is_checked: true
          data: 
            file: "../model/box1.body"
            format: CHOREONOID-BODY
            rootPosition: [ 1.00092508, 1.99978656, 1.49997979 ]
            rootAttitude: [ 
              0.999999993, -6.80843512e-06, 0.0001186464, 
              6.81239637e-06, 0.999999999, -3.33866841e-05, 
              -0.000118646172, 3.33874921e-05, 0.999999992 ]
            jointPositions: [  ]
            initialRootPosition: [ 1, 2, 1.5 ]
            initialRootAttitude: [ 
              1, 0, 0, 
              0, 1, 0, 
              0, 0, 1 ]
            staticModel: false
            collisionDetection: true
            selfCollisionDetection: false
            location_editable: true
            scene_sensitive: true
            zmp: [ 0, 0, 0 ]
        - 
          id: 11
          name: "cylinder1"
          plugin: Body
          class: BodyItem
          is_checked: true
          data: 
            file: "../model/cylinder1.body"
            format: CHOREONOID-BODY
            rootPosition: [ 1.99999604, 9.20266813e-07, 0.999900013 ]
            rootAttitude: [ 
              1, 9.66397127e-13, 1.0768042e-05, 
              1.0768042e-05, -1.7547519e-11, -1, 
              -9.66309311e-13, 1, -1.7547519e-11 ]
            jointPositions: [  ]
            initialRootPosition: [ 2, 0, 1 ]
            initialRootAttitude: [ 
              1, -6.71948619e-07, 9.44094884e-07, 
              9.44091993e-07, -4.30136943e-06, -1, 
              6.7195268e-07, 1, -4.3013688e-06 ]
            staticModel: false
            collisionDetection: true
            selfCollisionDetection: false
            location_editable: true
            scene_sensitive: true
            zmp: [ 0, 0, 0 ]
        - 
          id: 12
          name: "sphere1"
          plugin: Body
          class: BodyItem
          is_checked: true
          data: 
            file: "../model/sphere1.body"
            format: CHOREONOID-BODY
            rootPosition: [ -1.84369109, 2.65078124, 0.975802724 ]
            rootAttitude: [ 
              0.985574247, -0.0939606476, 0.140765051, 
              0.0699220412, 0.983488743, 0.166915551, 
              -0.154124336, -0.154665089, 0.975871098 ]
            jointPositions: [  ]
            initialRootPosition: [ -2, 2.5, 1 ]
            initialRootAttitude: [ 
              1, 0, 0, 
              0, 1, 0, 
              0, 0, 1 ]
            staticModel: false
            collisionDetection: true
            selfCollisionDetection: false
            location_editable: true
            scene_sensitive: true
            zmp: [ 0, 0, 0 ]
        - 
          id: 13
          name: "Cs137"
          plugin: Body
          class: BodyItem
          is_checked: true
          data: 
            file: "../model/Cs137.body"
            currentBaseLink: "CS137"
            format: CHOREONOID-BODY
            rootPosition: [ -0.5, 0.8, 0.5 ]
            rootAttitude: [ 
              1, -6.71788679e-09, 4.74033465e-07, 
              6.71591497e-09, 1, 4.15965644e-06, 
              -4.74033493e-07, -4.15965643e-06, 1 ]
            jointPositions: [  ]
            initialRootPosition: [ -0.5, 0.8, 0.5 ]
            initialRootAttitude: [ 
              1, -6.71788679e-09, 4.74033465e-07, 
              6.71591497e-09, 1, 4.15965644e-06, 
              -4.74033493e-07, -4.15965643e-06, 1 ]
            staticModel: true
